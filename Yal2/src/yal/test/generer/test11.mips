              .align 2
# Début du programme
.text
main :
	# Initialisation de s7 avec sp
	move $s7, $sp
	# Réservation de l'espace dans la pile
	addi $sp, $sp, -8
	# Chargement immédiat d'une constante entière
	li $v0, 0
	# Affectation simple
	sw $v0, 0($s7)
	# Chargement immédiat d'une constante entière
	li $v0, 0
	# Affectation simple
	sw $v0, -4($s7)
	# Chargement de la valeur de la variable dans v0
	lw $v0, -4($s7)
	# Affectation simple
	sw $v0, 0($s7)
	# affichage de l'expression
	# Chargement de la valeur de la variable dans v0
	lw $v0, 0($s7)
	move $a0, $v0
	li $v0, 1
	syscall
	li $v0, 4      # retour à la ligne
	la $a0, finLigne
	syscall
end :
