.data
finLigne:     .asciiz "\n"
              .align 2
.text
main :
                   # affichage de l'expression
    li $v0, 1
    move $a0, $v0
    li $v0, 1
    syscall
    li $v0, 4      # retour à la ligne
    la $a0, finLigne
    syscall
                   # affichage de l'expression
    li $v0, 5
    move $a0, $v0
    li $v0, 1
    syscall
    li $v0, 4      # retour à la ligne
    la $a0, finLigne
    syscall
                   # affichage de l'expression
    li $v0, 10
    move $a0, $v0
    li $v0, 1
    syscall
    li $v0, 4      # retour à la ligne
    la $a0, finLigne
    syscall
end :
    li $v0, 10            # retour au système
    syscall

