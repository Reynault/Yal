programme test debut
    fonction t(entier a; entier b; entier c) debut
        tantque a > b repeter
            si a == b+1 alors
                retourne a;
            sinon
                retourne b;
            finsi
        fintantque
        retourne c;
        ecrire 5;
        si a == c alors
            ecrire 50;
        sinon
            ecrire 5;
        finsi
    fin
    entier a;
    a = t(1,4,3);
    ecrire a;
fin